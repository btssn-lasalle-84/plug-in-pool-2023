class StringUtils
!!!155186.java!!!	inverse(in s : String) : String
        StringBuilder sb = new StringBuilder(s);
        for(int i = 0; i < sb.length() / 2; i ++)
        {
            char lettre = sb.charAt(i);
            sb.setCharAt(i, sb.charAt(sb.length() - i - 1));
            sb.setCharAt(sb.length() - i - 1, lettre); 
        }
        return (sb.toString());
